#Import constants
import sys, os
os_path = os.path.dirname(sys.path[0]).split("/")
del os_path[len(os_path) - 1]
if len(os_path) > 1 : sys.path.append(os.path.join("/".join(os_path),'constants'))
else: sys.path.append(os.path.join("/",'constants'))

import datetime, requests
from constants import API_ENDPOINT_PUBLISH

now = datetime.datetime.utcnow()
response = requests.get(url = API_ENDPOINT_PUBLISH)
response_json = response.json()
print("Se enviaron {} peticiones a las {}".format(response_json["count"], now)) 